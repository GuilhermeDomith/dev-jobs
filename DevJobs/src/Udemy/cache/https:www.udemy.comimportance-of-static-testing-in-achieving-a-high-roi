    <div class="recommendations" data-purpose="recommendations-unit">
        <span class="in-page-offset-anchor" data-purpose="recommendations-offset-anchor"></span>
        <div class="recommendations__course-cards ud-app-loader ud-component--course-comparison--app"
             data-module-id="course-comparison"
             data-module-args="{&quot;currentCourseId&quot;:627498,&quot;funnelTrackingSubcontext2&quot;:&quot;view-coursecomparison&quot;,&quot;discoveryContext&quot;:&quot;landing-page&quot;,&quot;funnelTrackingContext&quot;:&quot;landing-page&quot;,&quot;sourcePage&quot;:&quot;course_landing_page&quot;,&quot;funnelTrackingSubcontext&quot;:&quot;best-seller&quot;}">
            <div class="recommendations__content recommendations__content--course-comparison">
                <div class="recommendations__title">
                    <div class="content-loader__cc-content"></div>
                </div>
                <ul class="content-loader--list">
                    
                        <li class="content-loader__cc-row">
                            <span class="content-loader__cc-image">
                                <div class="content-loader__cc-content content-loader__cc-content--full-height"></div>
                            </span>
                            <span class="content-loader__cc-text">
                                <div class="content-loader__cc-content"></div>
                                <div class="content-loader__cc-content"></div>
                            </span>
                            <span class="content-loader__cc-stats">
                                <div class="content-loader__cc-content"></div>
                            </span>
                            <span class="content-loader__cc-stats">
                                <div class="content-loader__cc-content"></div>
                            </span>
                            <span class="content-loader__cc-stats">
