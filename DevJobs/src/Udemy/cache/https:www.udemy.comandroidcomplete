                            <div class="lecture-container 
                                        "
                                 data-purpose="lecture-item-2-2">
                                <div class="left-content">
                                    <span class="udi udi-play-circle"></span>
                                    <div class="top">
                                        
                                            <div class="description collapse"><p>Running An Android Application in Android Virtual Device and also in a Real Android Device</p></div>
                                        
                                        <div class="title">
                                            
                                                
                                                    Running an Android App in AVD and real device
                                                
                                            
                                            
                                                <a class="down-arrow" href="javascript:void(0)"
                                                   data-purpose="lecture-caret-2-2">
                                                    <i class="udi udi-caret-down"></i>
                                                </a>
                                            
                                        </div>
                                    </div>
