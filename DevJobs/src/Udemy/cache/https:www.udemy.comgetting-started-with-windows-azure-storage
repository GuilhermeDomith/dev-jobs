            <div class="col-xxs-8 left-col">
                <div class="clp-component-render">

    <div class="recommendations" data-purpose="recommendations-unit">
        <span class="in-page-offset-anchor" data-purpose="recommendations-offset-anchor"></span>
        <div class="recommendations__course-cards ud-app-loader ud-component--course-comparison--app"
             data-module-id="course-comparison"
             data-module-args="{&quot;currentCourseId&quot;:1231344,&quot;discoveryContext&quot;:&quot;landing-page&quot;,&quot;sourcePage&quot;:&quot;course_landing_page&quot;,&quot;funnelTrackingContext&quot;:&quot;landing-page&quot;,&quot;funnelTrackingSubcontext2&quot;:&quot;view-coursecomparison&quot;,&quot;funnelTrackingSubcontext&quot;:&quot;best-seller&quot;}">
