                                            <div class="description collapse"><p>#include&lt;iostream&gt; </p><p>using namespace std; </p><p></p><p>class Base { </p><p>public: </p><p>virtual void show() { cout&lt;&lt;" In Base \n"; } </p><p>}; </p><p></p><p>class Derived: public Base { </p><p>public: </p><p>void show() { cout&lt;&lt;"In Derived \n"; } </p><p>}; </p><p></p><p>int main(void) { </p><p>Base *bp = new Derived; </p><p>bp-&gt;show(); // RUN-TIME POLYMORPHISM </p><p>return 0; </p><p>}</p></div>
                                        
                                        <div class="title">
                                            
                                                
                                                    Program 4
                                                
                                            
                                            
                                                <a class="down-arrow" href="javascript:void(0)"
                                                   data-purpose="lecture-caret-7-4">
                                                    <i class="udi udi-caret-down"></i>
                                                </a>
                                            
                                        </div>
                                    </div>
                                </div>

                                <div class="details">
                                    
                                    <span class="content-summary">
                                        00:13
                                    </span>
                                </div>
