                StackExchange.ready(function () {
                    var $trigger = $('.js-more-actions-trigger'),
                        $dropdown = $('.js-more-actions-dropdown'),
                        $expressInterestTour = $('.js-job-interest-tour');

                    $trigger.on('click', function (e) {
                        e.preventDefault();
                        e.stopPropagation();
                        $dropdown.addClass('s-card__open');
                        $expressInterestTour.addClass('_hidden');
                    });
                    $(document).click(function () {
                        $dropdown.removeClass('s-card__open');
                    });
                    $('a', $dropdown).on('click', function () {
                        $dropdown.removeClass('s-card__open');
                    })
                });
            </script>
