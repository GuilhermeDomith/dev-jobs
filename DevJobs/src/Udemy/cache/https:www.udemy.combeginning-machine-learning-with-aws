                                 data-purpose="lecture-item-6-7">
                                <div class="left-content">
                                    <span class="udi udi-activity"></span>
                                    <div class="top">
                                        
                                        <div class="title">
                                            
                                                
                                                    Test Your Knowledge
                                                
                                            
                                            
                                        </div>
                                    </div>
                                </div>

                                <div class="details">
                                    
                                    <span class="content-summary">
                                        7 questions
                                    </span>
                                </div>
