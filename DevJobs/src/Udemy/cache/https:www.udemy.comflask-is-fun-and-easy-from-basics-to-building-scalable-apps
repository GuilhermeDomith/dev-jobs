    };
    window.ga.l = 1 * new Date();

    UD.GoogleAnalytics = UD.GoogleAnalytics || {};
    UD.GoogleAnalytics.createAccount = function (accountId, propertyName, domainName) {
        if (accountId) {
            if (propertyName) {
                // Required for multiple trackers
